\sl240\slmult1\ql\keep\widctlpar
\brdrb\brdrs\brdrw10\brsp40\li0\ri0\fi0\tqr\tx8787\par
\pard\plain\f0\fs20\b\expnd0\kerning0\cf0\sb0\sa0
\sl240\slmult1\ql\keep\widctlpar
\brdrb\brdrs\brdrw10\brsp40\li0\ri0\fi0\tqr\tx8787 Bank Merger (National/BNZ) Act\'a01997\u8212 ?28.5.2012\par
\pard\plain\f0\fs20\expnd0\kerning0\cf0\sb0\sa0
